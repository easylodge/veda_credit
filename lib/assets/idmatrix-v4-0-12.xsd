			<xs:element name="change-of-name-certificate-details" type="idm:change-of-name-certificate-detailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details on the change of name certificate
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="marriage-certificate-details" type="idm:marriage-certificate-detailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details on the marriage certificate
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="device-intelligence-details" type="idm:device-intelligence-detailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Details to collect a device intelligence
                        assessment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="knowledge-based-authentication-details" type="idm:knowledge-based-authentication-detailsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The fields needed to generate KBA questions or to provide the answers to KBA
                        questions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="global-screening-details" type="idm:global-screening-details" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Global Screening Detail</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="client-reference" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Optional identifier that can be used to correlate
                    this transaction with other records. This identifier is returned in
                    the response message.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="reason-for-enquiry" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Reason this lectronic verification is being
                    requested (anticipated future use for compliance)
                </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="enquiry-id" type="xs:token" use="optional">
			<xs:annotation>
				<xs:documentation>Previous enquiry identifiier for multi-pass
                    transactions</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="device-status" type="xs:string">
		<xs:annotation>
			<xs:documentation>The status of the device.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="policy-score" type="xs:int">
		<xs:annotation>
			<xs:documentation>The overall policy score for the device.
            </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="policy-score-reason-code" type="xs:string">
		<xs:annotation>
			<xs:documentation>The reason codes underlying the policy score.
            </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="device-intelligence-response">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:device-status"/>
				<xs:element ref="idm:policy-score"/>
				<xs:element ref="idm:policy-score-reason-code" maxOccurs="unbounded"/>
				<xs:element name="params-response" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Message element that contains a set of parameters
		            </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="param" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="idm:restricted-param">
										<xs:attribute name="key" type="xs:token" use="required"/>  
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>									
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>		
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="face-to-face-response">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="booking-url" type="xs:string" minOccurs="0"/>
				<xs:element name="manage-booking-url" type="xs:string" minOccurs="0"/>
			</xs:sequence>
			<xs:attributeGroup ref="idm:service-result-attributes"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="weight-type">
		<xs:annotation>
			<xs:documentation>A type to define all weightings used within the
                Fraud Assessment module. This only has two decimal places and allows
                weights from 0.00 to 1.00.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="3"/>
			<xs:fractionDigits value="2"/>
			<xs:maxInclusive value="1.00"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="score-type">
		<xs:annotation>
			<xs:documentation>A type to define all scores used within the Fraud
                Assessment module. This has one decimal place and allows values from
                0 to 100.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="4"/>
			<xs:fractionDigits value="1"/>
			<xs:maxInclusive value="100"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="threshold-type">
		<xs:annotation>
			<xs:documentation>A type to define all scores used within the Fraud
                Assessment module. This has one decimal place and allows values from
                0 to 100.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="4"/>
			<xs:fractionDigits value="1"/>
			<xs:maxInclusive value="100"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="fraud-result-type">
		<xs:annotation>
			<xs:documentation>Enumeration of results of fraud assessment for both
                the overall fraud assessment and the Assessment Factors.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="PASS">
				<xs:annotation>
					<xs:documentation>The enquiry was given a score greater than or
                        equal to the threshold for this criteria.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FAIL">
				<xs:annotation>
					<xs:documentation>The enquiry was given a score greater less than
                        the threshold for this criteria.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ERROR">
				<xs:annotation>
					<xs:documentation>An error occured while determining the score for
                        this criteria.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TIMEOUT">
				<xs:annotation>
					<xs:documentation>A timeout occured while executing the fraud assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOT_EXECUTED">
				<xs:annotation>
					<xs:documentation>Fraud assessment factor is not executed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="fraud-assessment-factor-type-type">
		<xs:annotation>
			<xs:documentation>Enumeration of values that the assessment-factor
                type can contain.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="DVI">
				<xs:annotation>
					<xs:documentation>The Device Intelligence assessment factor.
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KBA">
				<xs:annotation>
					<xs:documentation>The Knowledge Bases Authentication assessment
                        factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KYC_PEP">
				<xs:annotation>
					<xs:documentation>The KYC PolKnowledge Bases Authentication
                        assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KYC_SFD">
				<xs:annotation>
					<xs:documentation>The KYC Shared Fraud Bases Authentication
                        assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KYC_SNC">
				<xs:annotation>
					<xs:documentation>The KYC Sanctions Bases Authentication
                        assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KYC_VLC">
				<xs:annotation>
					<xs:documentation>The KYC Velocity Bases Authentication
                        assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PNV">
				<xs:annotation>
					<xs:documentation>The Phone Number Verification assessment factor.
                    </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SFA">
				<xs:annotation>
					<xs:documentation>The Second Factor Authentication assessment
                        factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WBC_BLOCK">
				<xs:annotation>
					<xs:documentation>Black list assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WBC_REFER">
				<xs:annotation>
					<xs:documentation>Refer list assessment factor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="fraud-assessment-factor-type">
		<xs:sequence>
			<xs:element name="type" type="idm:fraud-assessment-factor-type-type"/>
			<xs:element name="score" type="idm:score-type"/>
			<xs:element name="weight" type="idm:weight-type"/>
			<xs:element name="threshold" type="idm:threshold-type"/>
			<xs:element name="result" type="idm:fraud-result-type"/>
			<xs:element name="deal-breaker" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Describes whether the assessment factor is deal breaker or not.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="assessment-factors">
		<xs:annotation>
			<xs:documentation>The collection of assessment factors used to
                calculate the fraud assessment.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:assessment-factor" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="assessment-factor" type="idm:fraud-assessment-factor-type">
		<xs:annotation>
			<xs:documentation>Component Assessment factor used to calculate the
                fraud assessment.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="fraud-assessment-response">
		<xs:complexType>
			<xs:sequence minOccurs="0">
				<xs:element name="score" type="idm:score-type"/>
				<xs:element name="threshold" type="idm:threshold-type"/>
				<xs:element name="result" type="idm:fraud-result-type"/>
				<xs:element ref="idm:assessment-factors"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="phone-number" type="idm:phone-numberType">
		<xs:annotation>
			<xs:documentation>An Australian phone number with the leading zero
                and area code.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="phone-status" type="xs:string">
		<xs:annotation>
			<xs:documentation>The result of the verification of the phone number.
            </xs:documentation>
		</xs:annotation>
		<!-- Make this an enumeration -->
	</xs:element>
	<xs:element name="phone-verification-response">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:phone-number"/>
				<xs:element ref="idm:phone-status"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="phone-verification-responses">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:phone-verification-response" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="authentication-outcome" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>The result of the verification. This will be undertaken within IDM so no error value needs to be returned.
            </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="phone-authentication-response">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:phone-number"/>
				<xs:element ref="idm:authentication-outcome"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="phone-authentication-responses">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:phone-authentication-response" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="component-outcome-type">
		<xs:annotation>
			<xs:documentation>Overall outcome of the verification
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="component-name" type="idm:component-name-type">
				<xs:annotation>
					<xs:documentation>Outcomes Component Name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="indicator" type="idm:indicator">
				<xs:annotation>
					<xs:documentation>Classification of the verification-score in to an outcome category. Usually one of: accept, undecided or reject
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="total-points" type="idm:total-points">
				<xs:annotation>
					<xs:documentation>Total points for the verification
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="response-outcome-type">
		<xs:sequence>
			<xs:element name="overall-outcome" type="idm:indicator">
				<xs:annotation>
					<xs:documentation>Overall Outcome for Id Matrix Suite
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="component-outcomes" type="idm:component-outcome-type" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A break down of the components that created the
                        overall outcome
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="component-responses">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="idm:verification-response" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            verification
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="idm:fraud-assessment-response" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            fraud assessment
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="idm:phone-verification-responses" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            phone verification
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="idm:phone-authentication-responses" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            phone authentication
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="idm:device-intelligence-response" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            device intelligence
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="idm:knowledge-based-authentication-response" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            knowledge based authentication
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="idm:face-to-face-response" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Information associated with the execution of the
                            face to face request
                        </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="response-type">
		<xs:sequence>
			<xs:element name="response-outcome" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information regarding the outcome of the ID
                        Matrix operation
                    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="idm:response-outcome-type"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="idm:component-responses" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information regarding the breakdown of the
                        components
                    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="error" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information associated to application errors.
                    </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="idm:errorType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="verification-response">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>ComplexType for single
                    response messages
                </xs:documentation>
			</xs:annotation>
			<xs:sequence minOccurs="0">
				<xs:element name="verification-outcome">
					<xs:annotation>
						<xs:documentation>Overall outcome of the verification
                        </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="indicator" type="idm:indicator">
								<xs:annotation>
									<xs:documentation>Classification of the verification-score in to an outcome category. Usually one of: accept, undecided or reject
                                    </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="total-points" type="idm:total-points">
								<xs:annotation>
									<xs:documentation>Total points for the verification
                                    </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="self-verification-url" type="xs:anyURI" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="rules-results">
					<xs:annotation>
						<xs:documentation>Results of the rules/decision engine.
                        </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="rule-result" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Result of an individual rule
                                    </xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="indicator" type="idm:indicator">
											<xs:annotation>
												<xs:documentation>Classification of the verification-score in to an outcome category. Usually one of: accept, undecided or reject
                                                </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="reason" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Description of the reason for the decision indicator
                                                </xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:token"/>
											</xs:simpleType>
										</xs:element>
										<xs:element name="matrixrule" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Matrix rule which resulted in the decision.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="name" type="xs:token" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="analysis-results">
					<xs:annotation>
						<xs:documentation>Computed points from the raw search results
                        </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="analysis-result" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="category">
											<xs:annotation>
												<xs:documentation>Dataset search category this result belongs to
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:token"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="search-name" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Dataset search name used in this result
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:token"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="raw-score" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Raw match score returned by the search
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:int"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="minimum-value" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Minimum accept score for the search
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:int"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="filtered-score" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The score after filtering with the minimum-value
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:int"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="weight" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The weight applied to the filtered score to compute the points for this result
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:decimal"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="points" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The weighted points for this search.
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:int"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="contributing-factors">
											<xs:annotation>
												<xs:documentation>Factors contributing to the overall verification from this search
                                                </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name-match" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Name match was obtained in this search
